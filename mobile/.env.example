# REST + WebSocket endpoints the Expo app should call.
# These default to the production preview environment if left unspecified.
EXPO_PUBLIC_API_BASE_URL=https://endpoints.chatorbit.com/api
EXPO_PUBLIC_WS_BASE_URL=wss://endpoints.chatorbit.com

# Default ICE configuration used when no explicit overrides are provided.
EXPO_PUBLIC_WEBRTC_DEFAULT_STUN_URLS=stun:stun.nextcloud.com:443
EXPO_PUBLIC_WEBRTC_DEFAULT_TURN_URLS=turn:turn.yourserver.com:3478?transport=udp,turn:turn.yourserver.com:3478?transport=tcp,turns:turn.yourserver.com:443?transport=tcp
EXPO_PUBLIC_WEBRTC_TURN_DEFAULT_USER=youruser
EXPO_PUBLIC_WEBRTC_TURN_DEFAULT_PASSWORD=yourpassword

# Optional overrides that mirror the Next.js web client configuration.
# EXPO_PUBLIC_WEBRTC_STUN_URLS=stun:stun1.yourserver.com:3478,stun:stun2.yourserver.com:3478
# EXPO_PUBLIC_WEBRTC_TURN_URLS=turn:global.relay.metered.ca:80,turn:global.relay.metered.ca:443
# EXPO_PUBLIC_WEBRTC_TURN_USER=youruser
# EXPO_PUBLIC_WEBRTC_TURN_PASSWORD=yourpassword
# EXPO_PUBLIC_WEBRTC_ICE_SERVERS=[{"urls":["turn:turn.yourserver.com:3478"],"username":"demo","credential":"pass"}]
